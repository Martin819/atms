<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cz.polreich.banks">

    <permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="cz.polreich.banks.activity.HomeActivity"
            android:label="@string/app_name"
            android:permission="android.permission.INTERNET">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cz.polreich.banks.activity.BranchActivity"
            android:label="@string/title_activity_branch"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="cz.polreich.banks.activity.ATMActivity"
            android:label="@string/title_activity_atm"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="cz.polreich.banks.activity.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
    </application>

</manifest>